<?xml version="1.0" encoding="UTF-8"?>

<?import atlantafx.base.controls.*?>
<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane stylesheets="@custom-atlantafx.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.easytech.pelatologio.EditSubJobTeamController">
    <content>
        <VBox alignment="TOP_CENTER">
            <Label styleClass="title-label" text="Επεξεργασία Υποομάδας">
                <VBox.margin>
                    <Insets bottom="10.0" />
                </VBox.margin>
            </Label>
            <CustomTextField fx:id="nameField" promptText="Υποομάδα">
                <left>
                    <FontAwesomeIcon glyphName="USERS" />
                </left>
                <VBox.margin>
                    <Insets bottom="10.0" />
                </VBox.margin>
            </CustomTextField>
            <ComboBox fx:id="jobTeamField" promptText="Ομάδα" VBox.vgrow="ALWAYS">
                <VBox.margin>
                    <Insets />
                </VBox.margin>
            </ComboBox>
        </VBox>
    </content>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</DialogPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER"   xmlns="http://javafx.com/javafx/17.0.12"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.easytech.pelatologio.CustomerCallLogController">
    <HBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
        <VBox alignment="TOP_CENTER"  HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label text="Ιστορικό Κλήσεων"/>
            <ListView fx:id="callLogListView" VBox.vgrow="ALWAYS"/>
        </VBox>
    </HBox>
</VBox>


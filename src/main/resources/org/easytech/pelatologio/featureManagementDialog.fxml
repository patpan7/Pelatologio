                <Label styleClass="normal-label" text="Συνεργάτες/Προμήθειες:" GridPane.rowIndex="7" />
                <JFXCheckBox fx:id="partnersCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="7" />

                <Label layoutX="10.0" layoutY="346.0" styleClass="normal-label" text="Taxis:" GridPane.rowIndex="8" />
                <JFXCheckBox fx:id="taxisCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="8" />

                <Label styleClass="normal-label" text="myPOS:" GridPane.rowIndex="9" />
                <JFXCheckBox fx:id="myposCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="9" />

                <Label styleClass="normal-label" text="Simply:" GridPane.rowIndex="10" />
                <JFXCheckBox fx:id="simplyCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="10" />

                <Label styleClass="normal-label" text="Emblem:" GridPane.rowIndex="11" />
                <JFXCheckBox fx:id="emblemCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="11" />

                <Label styleClass="normal-label" text="Εργάνη:" GridPane.rowIndex="12" />
                <JFXCheckBox fx:id="erganiCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="12" />

                <Label styleClass="normal-label" text="Πελατολόγιο:" GridPane.rowIndex="13" />
                <JFXCheckBox fx:id="pelatologioCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="13" />

                <Label styleClass="normal-label" text="9POS:" GridPane.rowIndex="14" />
                <JFXCheckBox fx:id="nineposheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="14" />

                <Label styleClass="normal-label" text="Megasoft:" GridPane.rowIndex="15" />
                <JFXCheckBox fx:id="megasoftCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="15" />

                <Label styleClass="normal-label" text="Δ11:" GridPane.rowIndex="16" />
                <JFXCheckBox fx:id="d11Check" text="" GridPane.columnIndex="1" GridPane.rowIndex="16" />



                <columnConstraints>
                    <ColumnConstraints />
                    <ColumnConstraints />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
               <RowConstraints />
                    <RowConstraints />
                    <RowConstraints />
                </rowConstraints>
            </GridPane>
            <JFXButton buttonType="RAISED" onAction="#handleSave" styleClass="click-button" text="Αποθήκευση" />
        </VBox>
    </content>
</DialogPane>

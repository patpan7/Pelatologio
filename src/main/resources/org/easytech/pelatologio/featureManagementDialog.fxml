<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane stylesheets="@external.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.easytech.pelatologio.FeatureManagementDialogController">
    <content>
        <VBox spacing="10.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <Label styleClass="header-label" text="Διαχείριση Modules" />
            <GridPane hgap="10.0" vgap="10.0">
                <Label styleClass="normal-label" text="Προμηθευτές:" GridPane.rowIndex="0" />
                <JFXCheckBox fx:id="suppliersCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="0" />

                <Label styleClass="normal-label" text="Εργασίες:" GridPane.rowIndex="1" />
                <JFXCheckBox fx:id="tasksCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="1" />

                <Label styleClass="normal-label" text="Παραγγελίες:" GridPane.rowIndex="2" />
                <JFXCheckBox fx:id="ordersCheck" text="" GridPane.columnIndex="1" GridPane.rowIndex="2" />

